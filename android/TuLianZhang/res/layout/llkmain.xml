<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="horizontal" android:layout_width="fill_parent"
	android:layout_height="wrap_content">
	<Chronometer android:text="@+id/Chronometer01" android:id="@+id/Chronometer01"
		android:layout_width="wrap_content" android:layout_height="fill_parent"></Chronometer>
	<GridView android:id="@+id/llkGrid" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:numColumns="auto_fit"
